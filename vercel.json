{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "http://localhost:8000/api/",  
    "NEXT_PUBLIC_WS_URL": "ws://localhost:8000/ws/partners/"  
  }
}